pipeline {
  agent {
    kubernetes {
      defaultContainer 'jnlp'
      yaml """
apiVersion: v1
kind: Pod
metadata:
  labels:
    some-label: some-label-value
spec:
  containers:
  - name: ansible
    image: ansible:latest
    tty: true
"""
    }
  }
  stages {
    stage('Pull') {
      steps {
        container('maven') {
          sh 'mvn -version'
        }
      }
    }
  }
}